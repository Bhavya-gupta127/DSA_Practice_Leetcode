class   Solution   { 
 public : 
      bool   isValid ( string s )   { 
         stack < char > st ; 
          for ( auto  i : s ) 
          { 
              if ( ! st . empty ( ) ) 
              { 
                  if ( i == ')'   &&  st . top ( ) == '(' ) 
                     st . pop ( ) ; 
                  else   if ( i == '}'   &&  st . top ( ) == '{' ) 
                     st . pop ( ) ; 
                  else   if ( i == ']'   &&  st . top ( ) == '[' ) 
                     st . pop ( ) ; 
                  else 
                     st . push ( i ) ; 
              } 
              else 
                 st . push ( i ) ; 
          } 
          if ( ! st . empty ( ) )    
              return   false ; 
          return   true ; 
      } 
 } ;