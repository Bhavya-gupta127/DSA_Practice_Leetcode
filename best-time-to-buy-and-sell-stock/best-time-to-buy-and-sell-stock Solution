class   Solution   { 
 public : 
      int   maxProfit ( vector < int > &  prices )   { 
          int  ans = 0 ; 
          int  i = 0 , j = 0 ; 
          while ( j < prices . size ( ) ) 
          { 
              while ( j < prices . size ( )   &&  prices [ j ] >= prices [ i ] ) 
                 ans = max ( ans , prices [ j ++ ] - prices [ i ] ) ; 
             i = j ; 
          } 
          return  ans ; 
      } 
 } ;