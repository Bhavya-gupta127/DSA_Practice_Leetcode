class   Solution   { 
 public : 
      int   nextGreaterElement ( int  n )   { 
         string s = to_string ( n ) ; 
          next_permutation ( s . begin ( ) , s . end ( ) ) ; 
          long   long  temp = stoll ( s ) ; 
          if ( temp > INT_MAX  ||  temp <= n ) 
              return   - 1 ; 
          return  temp ; 
      } 
 } ;